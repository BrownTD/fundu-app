{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "target": "esnext",
    "lib": [
      "dom",
      "esnext"
    ],
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "frontendV2/**/*",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "frontendV2/babel.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
